{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": false,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es2015",
    "lib": ["dom", "es2015", "es2016", "es2017", "es2018", "es2019", "es2020"],
    "allowJs": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "noUnusedLocals": true
  },
  "exclude": [
    "./node_modules/*"
  ]
}
